{
  "name": "@sodular/lite",
  "version": "0.1.0",
  "description": "Sodular a Lite JSON database similar to firebase. Visit the master branch: https://github.com/coorise/sodular-lite-js.git",
  "type": "module",
  "main": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "unpkg": "dist/index.js",
  "publishConfig": {
    "ignore": [
      ".git/",
      ".idea/",
      ".backup/",
      "v*.zip/",
      "static/",
      "node_modules",
