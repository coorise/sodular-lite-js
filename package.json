  "author": "Agglomy",
  "license": "MIT",
  "contributors": [
    "Ivan Joel Sobgui"
  ],
  "dependencies": {
    "fs-extra": "^11.1.1",
    "JSONStream": "^1.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.6.5",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
